{
  "name": "js-kurzus-express",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "clean-client": "rm -rf build/assets",
    "clean-server": "rm -rf build/src",
    "start": "nodemon build/src/server/start-server.js",
    "build-client-dev": "npm run clean-client && webpack",
    "build-client-prod": "npm run clean-client && webpack --env.production",
    "build-client-dev:watch": "npm run clean-client && webpack --watch",
    "build-client-prod:watch": "npm run clean-client && webpack --env.production --watch",
    "build-server": "npm run clean-server && babel src/ --copy-files --out-dir build/src/ --ignore __tests__,.test.js,src/main.js,*.less,*.css",
    "build-server:watch": "npm run clean-server && babel src/ --copy-files --out-dir build/src/ --ignore __tests__,.test.js,src/main.js,*.less,*.css --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.5.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "6.26.0",
    "babel-jest": "22.4.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "body-parser": "^1.19.0",
    "compression-webpack-plugin": "^3.0.0",
    "cross-fetch": "^3.0.4",
    "cssnano": "^4.1.10",
    "express": "^4.17.1",
    "jest": "22.4.4",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^0.7.0",
    "nodemon": "^1.19.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5"
  },
  "devDependencies": {
    "css-loader": "^3.2.0",
    "jest": "^24.8.0",
    "prop-types": "^15.7.2",
    "style-loader": "^0.23.1"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/"
  }
